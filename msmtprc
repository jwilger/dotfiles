# gmail
account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from johnwilger@gmail.com
user johnwilger@gmail.com
tls on
tls_nocertcheck

# livingsocial
account livingsocial
host smtp.gmail.com
port 587
protocol smtp
auth on
from john.wilger@livingsocial.com
user john.wilger@hungrymachine.com
tls on
tls_nocertcheck

# use gmail as default
account default : gmail

